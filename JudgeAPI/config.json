{
  "app": {
    "debug": true,
    "host": "0.0.0.0",
    "port": 5000
  },
  "languages": {
    "python": {
      "extension": ".py",
      "runCommand": ["python3"],
      "compilation": false,
      "compilationCommand": null
    },
    "java": {
      "extension": ".java",
      "runCommand": ["java"],
      "compilation": true,
      "compilationCommand": ["javac"]
    },
    "csharp": {
      "extension": ".cs",
      "runCommand": ["dotnet", "run"],
      "compilation": true,
      "compilationCommand": ["dotnet", "build"]
    }
  }
}
